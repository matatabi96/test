package listsample;

public class MapSample {

	public static void main(String[] args) {
		Map<String,String> map =new HashMap<String,String>();

		//値のセットはputメソッドを利用
		map.put("key1","value1");
		map.put("key1","value2");
		map.put("key1","value3");
		map.put("key1","value4");
		map.put("key1","value5");

		//値の取得
		String value =map.get("key1"); //valueが取得できる
		System.out.println(value);


	}

}
