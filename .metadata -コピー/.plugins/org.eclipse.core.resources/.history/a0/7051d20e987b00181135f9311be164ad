drop database if exists sampledb;
create database sampledb;
use sampledb;

create table sample_table(
name varchar(255),
age int,
Mail varchar(255)
);

insert into sample_table values("Yamada",21,"xxx@hyaoo.co.jp");
insert into sample_table values("Sato",39,"yyy@google.com");
insert into sample_table values("Harada",44,"zzz@yahoo.co.jp");